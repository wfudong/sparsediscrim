    'dlda.r'
    'dqda.r'
    'eigendiag.r'
    'general-eigen.r'
    'joint-diagonalization.r'
    'jointDiag.r'
    'pool.r'
    'rsdda.r'
    'sdlda.r'
    'sdqda.r'
    'stein-shrinkage.r'
    'simdiag.r'
